{
	"geode": "4.3.1",
	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},
	"id": "techstudent10.gdguesser",
	"name": "GDGuesser",
	"version": "v1.0.0-beta.2",
	"developer": "TechStudent10",
	"description": "Guess the date of Geometry Dash levels!",
	"resources": {
		"sprites": [
			"resources/*.png"
		]
	},
	"settings": {
		"server-url": {
			"type": "string",
			"default": "https://gdguesser.underscored.me",
			"description": "The server to connect to",
			"name": "Server URL",
			"filter": "1234567890qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNMM~!@#$%^&*()-=[]\\;',./_+{}|:\"?"
		},
		"date-format": {
			"type":"string",
			"name": "Date Format",
			"default": "DD/MM/YYYY",
			"one-of": ["DD/MM/YYYY", "MM/DD/YYYY", "YYYY/MM/DD"],
			"description": "What date format to use, added this to cater to others worldwide (mostly Americans...)"
		},
		"dont-save-levels": {
			"type": "bool",
			"name": "Don't Save Levels",
			"default": false,
			"description": "Whether to save levels from your GDGuesser runs."
		}
	},
	"dependencies": {
		"geode.node-ids": "v1.20.1"
	}
}
